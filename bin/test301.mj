{

	char slovo(char a, int pom)
	{
		do
		{
			pom++;
			if(a == 'c' && pom < 10) continue;
			else if(a == 'b') break;
			else if( a == 'a') pom++;
			
			print(pom); print('.');
			do
			{
				pom++;
				if(pom == 12 || pom ==4 && a == 'a' || pom == 5) break;
				
				print('*');
				
			}
			while(pom < 5);
			
				print('<');print(a); print('>');
		
		}while(pom < 15 || pom == 16);
		return chr(pom + ord(a)) ;
	}

	int zamena(int niz[], int i, int b) 
		int pom; int pom2; 
		
	{
		if(i > len(niz) || b > len(niz) )
		{
		 	print('G'); return -1;
		}
		else
		{
				pom2 = niz[b];
				pom = niz[i];
				niz[b] = pom;
				niz[i] = pom2;
				return (pom ? pom2 + 50: pom2 + 40);
		} 
	
	}
	
	int faktorijel(int n)
	int pom; int pom2;
	{
		if(n > 0) pom = faktorijel(n - 1);
		if(n != 0 ) pom2 = pom * n;
		else if (n == 0) pom2 = 1;
		print(n,3);
		return pom2;
		
		
	}	
	
	void main()
	{	
		
